{
    "name": "moh-slimani/blade-fontawesome",
    "description": "A package to easily make use of FontAwesome in your Laravel Blade views.",
    "keywords": ["Blade", "FontAwesome", "Laravel"],
    "homepage": "https://github.com/moh-slimani/blade-fontawesome",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Slimani"
        }
    ],
    "require": {
        "php": "~8.2.0|~8.3.0",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "MohSlimani\\Fontawesome\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MohSlimani\\Fontawesome\\BladeFontawesomeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
